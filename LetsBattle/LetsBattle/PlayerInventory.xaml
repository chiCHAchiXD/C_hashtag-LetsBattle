<Window x:Class="LetsBattle.PlayerInventory"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LetsBattle"
        mc:Ignorable="d"
        Title="Inventory" Height="300" Width="399.74"
        ResizeMode="NoResize">
    <Grid>
        <Button x:Name="B_get_inventory" Content="Load Inventory" HorizontalAlignment="Left" Margin="294,10,0,0" VerticalAlignment="Top" Width="88" Height="47" Click="B_get_inventory_Click"/>
        <TextBox x:Name="Tb_inventory" HorizontalAlignment="Left" Height="249" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="166"/>

    </Grid>
</Window>
